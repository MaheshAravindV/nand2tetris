/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel, out=q);
    And(a=a, b=q, out=w);
    And(a=b, b=sel, out=e);
    Or(a=w, b=e, out=out);
}